{
  "class": {
    "prefix": "class",
    "body": "class=\"$1\"$0",
    "description": "Set class attribute"
  },
  "id": {
    "prefix": "id",
    "body": "id=\"$1\"$0",
    "description": "Set id attribute"
  },
  "data": {
    "prefix": "data",
    "body": "data-$1=\"$2\"$0",
    "description": "Set data attribute"
  },
  "aria": {
    "prefix": "aria",
    "body": "aria-$1=\"$2\"$0",
    "description": "Set aria attribute"
  },
  "role": {
    "prefix": "role",
    "body": "role=\"$1\"$0",
    "description": "Set role attribute"
  },
  "method": {
    "prefix": "method",
    "body": "method=\"$1\"$0",
    "description": "Set method attribute for form"
  },
  "action": {
    "prefix": "action",
    "body": "action=\"$1\"$0",
    "description": "Set action attribute for form"
  },
  "type": {
    "prefix": "type",
    "body": "type=\"$1\"$0",
    "description": "Set type attribute for form elements"
  },
  "name": {
    "prefix": "name",
    "body": "name=\"$1\"$0",
    "description": "Set name attribute for form elements"
  },
  "placeholder": {
    "prefix": "placeholder",
    "body": "placeholder=\"$1\"$0",
    "description": "Set placeholder attribute for form elements"
  },
  "value": {
    "prefix": "value",
    "body": "value=\"$1\"$0",
    "description": "Set value attribute for form elements"
  },
  "a": {
    "prefix": "a",
    "body": "a(href=\"${2:#}\", title=\"${3:title}\")${1:.class} $0",
    "description": "a(href=\"#\", title=\"title\").class"
  },
  "figure": {
    "prefix": "figure",
    "body": [
      "figure",
      "\timg(src=\"${2:source}\", alt=\"${3:alt}\")${1:.class}",
      "\tfigcaption $0"
    ],
    "description": "figure > img(src=\"source\", alt=\"alt\").class > figcaption"
  },
  "script": {
    "prefix": "script",
    "body": "script(src=\"${1:file}.js\")\n$0",
    "description": "script(src)"
  },
  "script.": {
    "prefix": "script:inline",
    "body": "script.\n\t$1\n$0",
    "description": "script."
  },
  "script include": {
    "prefix": "script:include",
    "body": "script\n\tinclude ${1:./file.js}\n$0",
    "description": "script -> include"
  },
  "link": {
    "prefix": "link",
    "body": "link(href=\"${1:file.css}\", rel=\"stylesheet\")\n$0",
    "description": "link(href,rel)"
  },
  "style": {
    "prefix": "style",
    "body": "style.\n\t$1\n$0",
    "description": "style."
  },
  "style include": {
    "prefix": "style:include",
    "body": "style\n\tinclude ${1:./file.css}\n$0",
    "description": "style -> include"
  },
  "var": {
    "prefix": "var",
    "body": "- var ${1:name} = $0",
    "description": "- var name = value"
  },
  "block": {
    "prefix": "block",
    "body": "block ${1:name}\n\t$2\n$0",
    "description": "block"
  },
  "case": {
    "prefix": "case",
    "body": "case ${1:variable}\n\twhen ${2:condition}\n\t\t${3:code}\n\tdefault\n\t\t${4:code}\n$0",
    "description": "case"
  },
  "each": {
    "prefix": "each",
    "body": "each ${1:val} in ${2:[}\n\t$3\n$0",
    "description": "each values"
  },
  "while": {
    "prefix": "while",
    "body": "while ${1:condition}\n\t$2\n$0",
    "description": "while"
  },
  "extends": {
    "prefix": "extends",
    "body": "extends ${1:./file.jade}\n$0",
    "description": "extends file"
  },
  "include": {
    "prefix": "include",
    "body": "include ${1:./file.jade}\n$0",
    "description": "inlude file"
  },
  "mixin": {
    "prefix": "mixin",
    "body": "mixin ${1:name}(${2:vars})\n\t$3\n$0",
    "description": "mixin"
  },
  "mixin:call": {
    "prefix": "mixin:call",
    "body": "+${1:name}(${2:attrs})${3:(${4:attrs})}\n$0",
    "description": "call mixin"
  },
  "anchor": {
    "prefix": "a",
    "body": "a(href=\"$1\") ${2:link}",
    "description": "anchor tag"
  },
  "image": {
    "prefix": "img",
    "body": "img(src=\"$1\", alt=\"${2:alt}\")",
    "description": "image tag"
  },
  "form": {
    "prefix": "form",
    "body": "form(action=\"$1\" method=\"${2:method}\")",
    "description": "form tag"
  },
  "lorem lipsum text": {
    "prefix": "lorem",
    "body": "Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.",
    "description": "insert dummy text"
  },
  "require pug": {
    "prefix": "cpug",
    "body": "const pug = require('pug');"
  },
  "import pug": {
    "prefix": "ipug",
    "body": "import pug from 'pug';"
  },
  "font-awesome icon": {
    "prefix": "fa",
    "body": "i.fa.fa-${1:value}(aria-hidden='true') &nbsp;",
    "description": "add a font-awesome icon"
  },
  "favicon": {
    "prefix": "favicon",
    "body": "link(rel=\"${1:shortcut-icon}\", href=\"${2:path}.ico\")",
    "description": "add a favicon"
  },
  "meta": {
    "prefix": "meta",
    "body": "meta(name=\"${1:name}\" content=\"${2:content}\")",
    "description": "add a meta tag"
  }
}
